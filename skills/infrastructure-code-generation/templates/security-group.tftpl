# modules/vpc/security-group.tf
# Security group with ingress and egress rules
# Generated by infrastructure-code-generation

resource "aws_security_group" "this" {
  name        = "${sg_name}"
  description = "${sg_description}"
  vpc_id      = "${vpc_id}"

  # Ingress rules
  ${ingress_rules}

  # Egress rules
  ${egress_rules}

  tags = merge(var.tags, {
    Name = "${sg_name}"
  })
}
