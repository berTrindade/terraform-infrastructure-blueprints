# Copy to .env and fill in. Do not commit .env.
# Required for production (https://mcp.ustwo.com:8443)

# MCP Server Configuration
NODE_ENV=production
MCP_TRANSPORT=http

# URLs - Note the :8443 port for independent deployment
MCP_BASE_URL=https://mcp.ustwo.com:8443
AUTH_BASE_URL=https://mcp.ustwo.com:8443

# Domain for OAuth
COMPANY_DOMAIN=ustwo.com

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# JWT Secret
JWT_SECRET=

# Optional
LOG_LEVEL=info
