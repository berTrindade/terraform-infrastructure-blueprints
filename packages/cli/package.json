{
  "name": "@bertrindade/agent-skills",
  "version": "0.1.0",
  "description": "CLI to install and manage skills for AI coding agents",
  "author": "bertrindade",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berTrindade/terraform-infrastructure-blueprints.git"
  },
  "type": "module",
  "bin": {
    "ustwo-skills": "./index.js"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "dev": "tsx src/index.ts",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "skills",
    "cli",
    "terraform",
    "infrastructure",
    "blueprints"
  ],
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "figlet": "^1.10.0",
    "gradient-string": "^3.0.0",
    "package-json": "^10.0.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "semantic-release": "^25.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
